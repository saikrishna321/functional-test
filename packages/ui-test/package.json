{
  "name": "ui-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check-format": "npx prettier --check .",
    "fix-format": "npx prettier --write .",
    "lint": "./node_modules/.bin/eslint .",
    "lint-fix": "./node_modules/.bin/eslint . --fix",
    "install-cm": "curl -s https://aerokube.com/cm/bash | bash",
    "selenoid:download": "./cm selenoid download",
    "selenoid:configure": "./cm selenoid configure --browsers chrome:96.0 -f",
    "selenoid:start": "./cm selenoid start",
    "selenoid:stop": "./cm selenoid stop",
    "selenoid:cleanup": "./cm selenoid cleanup",
    "pretest": "npm run selenoid:download && npm run selenoid:configure && npm run selenoid:start",
    "aftertest": "npm run selenoid:cleanup && npm run selenoid:stop && npm run selenoid:cleanup",
    "web": "npx wdio ./src/config/local/wdio.web.conf.js",
    "web:docker": "npx wdio ./src/config/local/wdio.web.docker.conf.js && npm run aftertest",
    "postinstall": "(npx appium driver install uiautomator2 || exit 0) && (npx appium driver install xcuitest || exit 0)",
    "android": "npx wdio ./src/config/local/wdio.android.conf.js",
    "ios": "npx wdio ./src/config/local/wdio.ios.conf.js",
    "cloud:android": "npx wdio ./src/config/cloud/wdio.bs.android.conf.js",
    "cloud:ios": "npx wdio ./src/config/cloud/wdio.bs.ios.conf.js",
    "cloud:web": "npx wdio ./src/config/cloud/wdio.bs.web.conf.js"
  },
  "devDependencies": {
    "@wdio/appium-service": "7.16.11",
    "@babel/register": "7.16.5",
    "@wdio/cli": "7.16.12",
    "@wdio/local-runner": "7.16.12",
    "@wdio/mocha-framework": "7.16.11",
    "@wdio/sync": "7.16.12",
    "chromedriver": "96.0.0",
    "global-agent": "3.0.0",
    "pinst": "^2.1.6",
    "wdio-chromedriver-service": "7.2.2",
    "wdio-timeline-reporter": "5.1.4",
    "webdriverio": "7.16.12",
    "sc-utils": "1.0.0",
    "appium": "2.0.0-beta.23"
  },
  "author": "",
  "license": "ISC"
}
